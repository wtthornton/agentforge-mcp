# .github/workflows/cursor-init.yml
name: Cursor Init Process
on:
  push:
    paths:
      - '.agent-os/**'
  schedule:
    - cron: '0 0 * * *' # Daily

jobs:
  cursor-init:
    runs-on: ubuntu-latest
    steps:
      - uses: actions/checkout@v3
      - uses: actions/setup-node@v3
        with:
          node-version: '18'
      - run: npm install
      - run: npm run cursor:init
      - run: git add .cursor/
      - run: git commit -m "Update Cursor rules" || true
      - run: git push || true
