{
  "noPromptCommands": {
    "maven": {
      "mvn test": false,
      "mvn test -Dtest=DatabaseConnectionTest,DatabaseInitializationTest": false,
      "mvn compile": false,
      "mvn package": false,
      "mvn install": false,
      "mvn clean": false,
      "mvn spring-boot:run": false
    },
    "java": {
      "java -version": false,
      "java -jar": false,
      "java -cp": false,
      "javac": false
    },
    "packageManagement": {
      "apt list --installed": false,
      "apt list --installed | grep -i openjdk": false,
      "apt update": false,
      "apt upgrade": false,
      "apt install": false,
      "apt-get update": false,
      "apt-get upgrade": false,
      "apt-get install": false,
      "dpkg -l": false,
      "dpkg -l | grep -i openjdk": false
    },
    "systemCommands": {
      "which java": false,
      "which mvn": false,
      "which node": false,
      "which npm": false,
      "which git": false,
      "which docker": false,
      "env | grep JAVA": false,
      "env | grep MAVEN": false,
      "echo $JAVA_HOME": false,
      "echo $MAVEN_HOME": false,
      "pwd": false,
      "ls -la": false,
      "cd *": false,
      "mkdir *": false,
      "rm *": false,
      "cp *": false,
      "mv *": false
    },
    "developmentTools": {
      "node *": false,
      "npm *": false,
      "yarn *": false,
      "pnpm *": false,
      "git *": false,
      "docker *": false,
      "docker-compose *": false
    }
  },
  "globalSettings": {
    "disableAllPrompts": true,
    "autoExecuteCommands": true,
    "autoExecuteBash": true,
    "autoExecuteMaven": true,
    "autoExecuteJava": true,
    "autoExecutePackageManagement": true,
    "autoExecuteSystemCommands": true,
    "autoExecuteDevelopmentTools": true
  },
  "shellTypes": {
    "bash": {
      "enabled": true,
      "noPrompt": true,
      "autoExecute": true
    },
    "powershell": {
      "enabled": true,
      "noPrompt": true,
      "autoExecute": true
    },
    "cmd": {
      "enabled": true,
      "noPrompt": true,
      "autoExecute": true
    },
    "wsl": {
      "enabled": true,
      "noPrompt": true,
      "autoExecute": true
    }
  }
}
